{
  "expo": {
    "name": "ShakeIt",
    "slug": "shakeit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "shakeit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#43C3A2",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nz.shakeit.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ShakeIt uses your location to show nearby milkshake shops.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#43C3A2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "nz.shakeit.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#43C3A2",
          "dark": {
            "backgroundColor": "#118067"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f76dc992-556e-4944-b4ec-3c242771fe2f"
      }
    }
  }
}
